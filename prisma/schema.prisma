datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Funcionario {
  id        Int      @id @default(autoincrement())
  nome      String
  cargo     String
  email     String   @unique
  senha     String
  permissao String
  updatedAt DateTime @updatedAt
  createdAt DateTime @default(now())
}
